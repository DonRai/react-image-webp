{
  "name": "react-image-webp",
  "version": "0.8.0",
  "description": "React component for check browser support format webp and insert if supported webp image.",
  "repository": {
    "type": "git",
    "url": "https://github.com/DonRai/react-image-webp"
  },
  "keywords": [
    "react",
    "webp",
    "images"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "babel modules/ --out-dir dist/",
    "minify": "minify dist -d dist",
    "size-limit": "size-limit"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "3.30 KB"
    },
    {
      "path": "dist/utils/index.js",
      "limit": "190 B"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^6.0.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-minify": "^0.5.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1"
  },
  "dependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  }
}
